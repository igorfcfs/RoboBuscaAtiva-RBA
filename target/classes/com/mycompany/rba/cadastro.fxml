<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="236.0" prefWidth="414.0" spacing="20.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.mycompany.rba.CadastroController">
    <children>
        <Label text="Faça seu Cadastro" />
      <HBox prefHeight="25.0" prefWidth="282.0">
         <children>
            <Label prefHeight="25.0" prefWidth="135.0" text="Email Institucional" />
            <TextField />
         </children>
      </HBox>
      <HBox prefHeight="25.0" prefWidth="282.0">
         <children>
            <Label prefHeight="25.0" prefWidth="135.0" text="Senha" />
            <TextField />
         </children>
      </HBox>
      <HBox prefHeight="25.0" prefWidth="282.0">
         <children>
            <Label prefHeight="25.0" prefWidth="135.0" text="Confirmar Senha" />
            <TextField />
         </children>
      </HBox>
        <Button fx:id="secondaryButton" onAction="#cadastrar" text="Cadastrar" />
      <HBox prefHeight="100.0" prefWidth="200.0">
         <children>
            <Label prefHeight="17.0" prefWidth="117.0" text="Já tem uma conta?" />
            <Button fx:id="secondaryButton1" onAction="#entrar" text="Entrar" />
         </children>
      </HBox>
    </children>
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
</VBox>
