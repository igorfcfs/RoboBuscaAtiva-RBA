<VBox alignment="CENTER" prefHeight="236.0" prefWidth="414.0" spacing="20.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.mycompany.rba.CentralAutomacoes">
    <children>
      <HBox alignment="TOP_RIGHT" prefHeight="25.0" prefWidth="374.0">
         <children>
              <Label prefHeight="27.0" prefWidth="308.0" text="Central de Automações">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <Button fx:id="btnAdicionar" mnemonicParsing="false" onAction="#minhaEscola" text="Minha Escola" />
         </children>
      </HBox>
      <Button fx:id="secondaryButton1" onAction="#executar" prefHeight="25.0" prefWidth="88.0" text="Executar" />
    </children>
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
</VBox>
